  <property name="joint_finger_tip_1_origin_xyz" value="0.035 -0.02595 -0.0025" />
  <property name="joint_finger_tip_1_origin_rpy" value="0 0 0" />
  <property name="joint_finger_tip_1_lower_limit" value="0" />
