  <property name="joint_finger_tip_3_axis_xyz" value="0 0 1" />
  <property name="joint_finger_tip_3_origin_xyz" value="0.035 -0.02595 -0.0025" />
  <property name="joint_finger_tip_3_origin_rpy" value="0 0 0" />
  <property name="joint_finger_tip_3_lower_limit" value="0" />
  <property name="joint_finger_tip_3_upper_limit" value="${J_PI/180*40}" />
  <property name="joint_finger_tip_3_mimic_joint" value="${J_PI/180*40}" />
  <property name="joint_finger_tip_3_mimic_multiplier" value="0.0" />
  <property name="joint_finger_tip_3_mimc_offset" value="0.0" />

  <property name="link_end_effector"   value="jaco2_end_effector" />
  <property name="end_effector_offset" value="0.1687" />

  <xacro:macro name="jaco2_arm" params="parent">

    <xacro:kinova_6DOF base_parent="${parent}"/>

    <link name="${link_end_effector}" />
    <joint name="${link_6}_to_end_effector" type="fixed">
      <parent link="${link_6}"/>
      <child link="${link_end_effector}"/>
      <origin xyz="0 0 -${end_effector_offset}" rpy="0 ${J_PI} 0" />
    </joint>

    <xacro:kinova_3fingers link_hand="${link_6}"/>

  </xacro:macro>

</root>
