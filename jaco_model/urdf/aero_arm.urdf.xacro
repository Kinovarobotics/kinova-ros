        rpy="0 0 0" />
      <mass
        value="0.001" />
      <inertia
        ixx="0.001"
        ixy="0.001"
        ixz="0.001"
        iyy="0.001"
        iyz="0.001"
        izz="0.001" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://aero_arm/meshes/base_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://aero_arm/meshes/base_link.STL" />
      </geometry>
    </collision>
  </link>
  <link
