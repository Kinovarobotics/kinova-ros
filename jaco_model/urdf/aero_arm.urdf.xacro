      </geometry>
      <material
        name="">
        <color
          rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="1.57 0 0" />
      <geometry>
        <mesh
          filename="package://aero_arm/meshes/Link1.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="${prefix}_Joint1"
    type="continuous">
    <origin
      xyz="0 0 0"
      rpy="0 0 0" />
